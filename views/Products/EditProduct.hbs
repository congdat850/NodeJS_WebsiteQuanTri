        </div>
        <!-- Single pro tab review Start-->
        <div class="single-pro-review-area mt-t-30 mg-b-15">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="product-payment-inner-st">
                            <ul id="myTabedu1" class="tab-review-design">
                                <li class="active"><a href="#description">Chỉnh sửa sản phẩm</a></li>
                            </ul>
                            <div id="myTabContent" class="tab-content custom-product-edit">
                                <div class="product-tab-list tab-pane fade active in" id="description">
                                    <div class="row">
                                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                            <div class="review-content-section">
                                                <div id="dropzone1" class="pro-ad addcoursepro">
                                                    <form method="POST" class="dropzone dropzone-custom needsclick addcourse" id="demo1-upload" enctype="multipart/form-data">
                                                        <div class="row">
                                                            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                                                <div class="form-group">
                                                                    <label>ID sản phẩm</label>
                                                                    <input name="productID" type="text" class="form-control" placeholder="ProductID" value="{{data.productID}}">
                                                                </div>
                                                                <div class="form-group">
                                                                    <label>Tên sản phẩm</label>
                                                                    <input name="name" type="text" class="form-control" placeholder="Tên sản phẩm" value="{{data.name}}">
                                                                </div>
                                                                <div class="form-group">
                                                                    <label>Giá ban đầu</label>
                                                                    <input name="oldPrice" type="text" class="form-control" placeholder="Giá ban đầu của sản phẩm" value="{{data.oldPrice}}">
                                                                </div>
                                                                <div class="form-group">
                                                                    <label>Giá mới</label>
                                                                    <input name="newPrice" type="text" class="form-control" placeholder="Giá mới của sản phẩm" value="{{data.newPrice}}">
                                                                </div>
                                                                <div class="form-group">
                                                                    <label>Gian hàng</label>
                                                                    <select name="producer" class="form-control">
																			<option value="{{data.producer}}" selected="" disabled="">{{data.producer}}</option>
                                                                            {{#each gianHang}}
																			<option value="{{this.name}}">{{this.name}}</option>
                                                                            {{/each}}
																		</select>
                                                                </div>
                                                                <div class="form-group">
                                                                  <label for="AnhSanPham">Ảnh sản phẩm</label>
                                                                  <input type="file" class="form-control-file" name="AnhSanPham" id="" placeholder="" aria-describedby="fileHelpId">
                                                                  <small id="fileHelpId" class="form-text text-muted">Click để upload file</small>
                                                                </div>
                                                            </div>
                                                            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12"> 
                                                                <div class="form-group">
                                                                    <label>Số lượng sản phẩm</label>
                                                                    <input name="quantitySold" type="number" class="form-control" placeholder="Số lượng sản phẩm" value="{{data.quantitySold}}">
                                                                </div>
                                                                <div class="form-group">
                                                                    <label>Nơi sản xuất</label>
                                                                    <input name="country" type="text" class="form-control" placeholder="Nơi sản xuất" value="{{data.country}}">
                                                                </div>
                                                                <div class="form-group">
                                                                    <label>Số sao đánh giá</label>
                                                                    <input name="starEvaluate" type="number" class="form-control" placeholder="Số sao đánh giá" value="{{data.starEvaluate}}">
                                                                </div>
                                                                <div class="form-group">
                                                                    <label>Ngày mở bán</label>
                                                                    <input name="dateUpdate" type="text" class="form-control" placeholder="Ngày mở bán" value="{{data.dateUpdate}}">
                                                                </div>

                                                                <div class="form-group">
                                                                    <label>Miêu tả sản phẩm</label>
                                                                    <textarea name="description" placeholder="Miêu tả sản phẩm" value="{{data.description}}">{{data.description}}</textarea>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="row">
                                                            <div class="col-lg-12">
                                                                <div class="payment-adress">
                                                                    <button type="submit" class="btn btn-primary waves-effect waves-light">Hoàn tất chỉnh sửa</button>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </form>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>