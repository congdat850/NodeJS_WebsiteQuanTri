<div class="breadcome-area">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="breadcome-list single-page-breadcome">
                                <div class="row">
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                        <div class="breadcome-heading">
                                            <form role="search" class="sr-input-func">
                                                <input type="text" placeholder="Search..." class="search-int form-control">
                                                <a href="#"><i class="fa fa-search"></i></a>
                                            </form>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                        <ul class="breadcome-menu">
                                            <li><a href="#">Home</a> <span class="bread-slash">/</span>
                                            </li>
                                            <li><span class="bread-blod">Thêm sản phẩm</span>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Single pro tab review Start-->
        <div class="single-pro-review-area mt-t-30 mg-b-15">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="product-payment-inner-st">
                            <ul id="myTabedu1" class="tab-review-design">
                                <li class="active"><a href="#description">Thông tin sản phẩm</a></li>
                                
                            </ul>
                            <div id="myTabContent" class="tab-content custom-product-edit">
                                <div class="product-tab-list tab-pane fade active in" id="description">
                                    <div class="row">
                                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                            <div class="review-content-section">
                                                <div id="dropzone1" class="pro-ad addcoursepro"></div>
                                                    <form method="POST" class="dropzone dropzone-custom needsclick addcourse" id="demo1-upload" enctype="multipart/form-data">
                                                        <div class="row">
                                                            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                                                <div class="form-group">
                                                                    <label>ID sản phẩm</label>
                                                                    <input name="productID" type="text" required minlength="6" maxlength="20" class="form-control" placeholder="ProductID">
                                                                </div>
                                                                <div class="form-group">
                                                                    <label>Tên sản phẩm</label>
                                                                    <input name="name" type="text" required minlength="6" maxlength="20" class="form-control" placeholder="Tên Sản phẩm">
                                                                </div>
                                                                <div class="form-group">
                                                                    <label>Giá ban đầu</label>
                                                                    <input name="oldPrice" type="text" required  class="form-control" placeholder="Giá ban đầu của sản phẩm">
                                                                </div>
                                                                <div class="form-group">
                                                                    <label>Giá mới</label>
                                                                    <input name="newPrice" type="text" required class="form-control" placeholder="Giá mới của sản phẩm">
                                                                </div>
                                                              
                                                                <div class="form-group">
                                                                    <label>Gian hàng</label>
                                                                    <select name="producer" class="form-control">
                                                                        {{#each data}}
																		<option value="{{this.name}}">{{this.name}}</option>
																		{{/each}}
																	</select>
                                                                </div>
                                                                <div class="form-group">
                                                                  <label for="AnhSanPham">Ảnh sản phẩm</label>
                                                                  <input type="file" required class="form-control-file" name="AnhSanPham" id="" placeholder="" aria-describedby="fileHelpId">
                                                                  <small id="fileHelpId" class="form-text text-muted">Click để upload file</small>
                                                                </div>
                                                            </div>
                                                            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                                                <div class="form-group">
                                                                    <label>Số lượng sản phẩm</label>
                                                                    <input name="quantitySold" required type="number" class="form-control" placeholder="Số lượng sản phẩm">
                                                                </div>
                                                                <div class="form-group">
                                                                    <label>Nơi sản xuất</label>
                                                                    <input name="country" required minlength="6" maxlength="20" type="text" class="form-control" placeholder="Nơi sản xuất">
                                                                </div>
                                                                <div class="form-group">
                                                                    <label>Số sao đánh giá</label>
                                                                    <select name="starEvaluate" class="form-control">
																		<option value="0">5</option>
																		<option value="1">4</option>
                                                                        <option value="2">3</option>
                                                                        <option value="3">2</option>
                                                                        <option value="4">1</option>
																	</select>
                                                                </div>
                                                                <div class="form-group">
                                                                    <label>Ngày mở bán</label>
                                                                    <input name="dateUpdate" required type="text" class="form-control" placeholder="Ngày mở bán">
                                                                </div>
                                                                <div class="form-group">
                                                                    <label>Miêu tả sản phẩm</label>
                                                                    <textarea name="description" required minlength="6" maxlength="2000" placeholder="Miêu tả sản phẩm"></textarea>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="row">
                                                            <div class="col-lg-12">
                                                                <div class="payment-adress">
                                                                    <button type="submit" class="btn btn-primary waves-effect waves-light">Thêm sản phẩm</button>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </form>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>