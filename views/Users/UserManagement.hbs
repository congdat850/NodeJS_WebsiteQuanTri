{{#if Notify}}
        <script>alert("Đăng kí thành công");</script>
    {{/if}}
<div class="breadcome-area">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="breadcome-list single-page-breadcome">
                                <div class="row">
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                        <div class="breadcome-heading">
                                            <form role="search" class="sr-input-func">
                                                <input type="text" placeholder="Search..." class="search-int form-control">
                                                <a href="#"><i class="fa fa-search"></i></a>
                                            </form>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                        <ul class="breadcome-menu">
                                            <li><a href="#">Home</a> <span class="bread-slash">/</span>
                                            </li>
                                            <li><span class="bread-blod">Danh sách người dùng</span>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="product-status mg-b-15">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="product-status-wrap drp-lst">
                            <h4>Danh sách người dùng</h4>
                            <div class="add-product">
                                <a href="/ThemNguoiDung">Thêm người dùng</a>
                            </div>
                            <div class="asset-inner">
                                <table>
                                    <tr>
                                        <th>STT</th>
                                        <th>Tên người dùng</th>
                                        <th>Mức quyền</th>
                                        <th>Email</th>
                                        <th>Số điện thoại</th>
                                        <th>Chỉnh sửa</th>
                                    </tr>
                                    {{#each data}}
                                    <tr>
                                        <td>{{@index}}</td>
                                        <td>{{this.userName}}</td>
                                        <td>{{this.authority}}</td>
                                        <td>{{this.email}}</td>
                                        <td>{{this.phoneNumber}}</td>
                                        
                                        <td>
                                            <a href="/ChinhSuaNguoiDung?id={{this._id}}"><button data-toggle="tooltip" title="Chỉnh sửa" class="pd-setting-ed"><i class="fa fa-pencil-square-o" aria-hidden="true"></i></button></a>
                                            <a href="/XoaNguoiDung/{{this.id}}"> <button data-toggle="tooltip" title="Trash" class="pd-setting-ed"><i class="fa fa-trash-o" aria-hidden="true"></i></button></a>
                                            <a href="/ThongTinChiTiet?id={{this._id}}"> <button data-toggle="tooltip" title="xem" class="pd-setting-ed"><i class="far fa-address-card"></i></button></a>
                                             <span ></span>
                                        </td>
                                    </tr>
                                    {{/each}}
                                </table>
                            </div>
                           <div class="row">
                               <div class="col-md-12">
                                 <hd-paging current-page="{{current}}" max-page="{{pages}}" for="TatCaNguoiDung"></hd-paging>
                              </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
<script>
$(document).ready(function(){
    $('ul.navbar-nav li').removeClass('active');
    $('li.category').addClass('active');
})
</script>