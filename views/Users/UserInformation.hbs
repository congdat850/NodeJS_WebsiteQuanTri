<div class="breadcome-area">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="breadcome-list">
                                <div class="row">
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                        <div class="breadcome-heading">
                                            <form role="search" class="sr-input-func">
                                                <input type="text" placeholder="Search..." class="search-int form-control">
                                                <a href="#"><i class="fa fa-search"></i></a>
                                            </form>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                        <ul class="breadcome-menu">
                                            <li><a href="#">Home</a> <span class="bread-slash">/</span>
                                            </li>
                                            <li><span class="bread-blod">All Courses</span>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="courses-area">
            <div class="container-fluid">
                <div class="row">
                     {{#each data}}
                    <div class="col-lg-3 col-md-6 col-sm-6 col-xs-12">
                        <div class="courses-inner mg-t-30">
                            <div class="courses-title">
                                <a href="#"><img src="images/DaiDienMacDinh.jpg" alt=""></a>
                                <h2>{{this.userName}}</h2>
                            </div> 
                            <div class="course-des">
                                <p><span><i class="fas fa-award"></i></span> <b>Mức Quyền: </b>{{this.authority}} </p>
                                <p><span><i class="fas fa-envelope-open-text"></i></span> <b>Email: </b>{{this.email}}</p>
                                <p><span><i class="fas fa-mobile-alt"></i></span> <b>Số điện thoại: </b>{{this.phoneNumber}}</p>
                            </div>
                            <div class="product-buttons">
                                <a href="/ChinhSuaNguoiDung?id={{this._id}}"> <button type="button" class="button-default cart-btn">Chỉnh sửa</button></a>
                                <a href="/XoaNguoiDung/{{this.id}}"><button type="button" class="button-default cart-btn">Xóa</button></a>                     
                            </div>
                        </div>
                    </div>
                    {{/each}}
                </div>
            </div>
        </div>